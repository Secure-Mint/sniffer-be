  "scripts": {
    "build": "yarn run barrels && swc src --out-dir dist -s  --strip-leading-paths",
    "barrels": "barrels",
    "dev": "yarn run barrels && nodemon src/index.ts",
    "start": "cross-env NODE_ENV=production node --import @swc-node/register/esm-register src/index.ts",
    "test": "yarn run test:lint",
    "prisma:migrate": "npx prisma migrate dev && yarn prisma:generate",
    "prisma:migrate:reset": "npx prisma migrate reset",
    "prisma:generate": "npx prisma generate",
    "test:lint": "eslint",
    "test:lint:fix": "eslint --fix",
    "prettier": "prettier '**/*.{json,md,yml,yaml}' --write",
    "cron:tokens:run": "node --import @swc-node/register/esm-register src/cron/tokens/index.ts"
  },
